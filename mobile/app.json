{
  "expo": {
    "name": "DecorArte",
    "slug": "DecorArte",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ed0059"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
