    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ed0059"
      },
      "package": "br.com.alexsouzasilvax.decorarte"
    },
    "primaryColor": "#ed0059"
  }
}
